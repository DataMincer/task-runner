  /** @noinspection PhpIncludeInspection */
  require $file;
}
else {
  die("Can't find autoload.php");
}

/** Replace 'TaskRunner\App' class with your version of the App class, e.g. MyProject\App() */
(new TaskRunner\App())->run();
